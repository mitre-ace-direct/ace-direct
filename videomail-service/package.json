{
  "name": "videomail-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "audit-fix": "npm audit fix",
    "test": "echo 'no tests.'",
    "preinstall": "git config --global url.\"https://\".insteadOf git://",
    "postinstall": "cp mods/jssip/* node_modules/jssip/lib-es5/",
    "start": "node ./src/server.js",
    "clean": "rm -rf node_modules >/dev/null 2>&1 || true",
    "clean:logs": "rm logs/*.log* > /dev/null 2>&1 || true",
    "lint": "find . -name '*.js'  -not -path './node_modules/*' -not -path './public/assets/js/*' -not -path './.cache/*' -not -path './bower_components/*' -not -path './public/js/*' -print0 | xargs -0 eslint",
    "build": "npm run clean && npm install"
  },
  "dependencies": {
    "aws-sdk": "^2.910.0",
    "connect": "^3.7.0",
    "crypto": "^1.0.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "finalhandler": "^1.1.2",
    "form-data": "^3.0.0",
    "get-video-duration": "^3.0.1",
    "jssip": "^3.3.6",
    "jssip-node-websocket": "^3.0.0",
    "kurento-client": "^6.10.0",
    "lodash": "^4.17.12",
    "lodash.merge": "^4.6.2",
    "mini-stack": "^1.0.6",
    "minimist": "^1.2.3",
    "mysql": "^2.17.1",
    "nconf": "^0.11.4",
    "proxy-agent": "^4.0.1",
    "redis": "^3.0.2",
    "scp2": "^0.5.0",
    "sdp-transform": "^2.8.0",
    "serve-static": "^1.14.1",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4"
  },
  "description": "built off https://github.com/agilityfeat/kurento-asterisk",
  "main": "server.js",
  "author": "",
  "license": "ISC"
}
