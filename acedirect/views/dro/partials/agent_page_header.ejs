<style>
  #consumerNavbar {
    width: 100%;
    display: flex;
  }

  .display-flex-center {
    display: flex;
    align-items: center;
    width: 100%;
    /* min-width: 992px; */
    margin-left: 0px;
  }

  .Navbar {
    background-color: #073863;
    color: #FFFFFF;
    padding: .6em 30px .6em 30px;
  }

  .appTitle {
    font-size: 32px;
    font-weight: bold;
    padding-left: 0px !important;
    padding-right: 0px !important;
  }

  @media (max-width: 500px) {
    #consumerNavbar {
      justify-content: center;
      text-align: center;
    }

    .appTitle {
      font-size: 1.4em;
    }
  }

</style>


  <div id="consumerNavbar" class="Navbar row display-flex-center">
    <div id="navText" class="appTitle col-xs-12 col-md-6"></div>
  </div>

  <script>
    let consumerPortalTitle = '<%= consumerPortalTitle %>'
    window.addEventListener('load', () => {
      document.getElementById('navText').innerText = consumerPortalTitle;
    });
  </script>

