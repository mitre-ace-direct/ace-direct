{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "comma-dangle": ["error", "never"],
      "no-unused-vars": [2, {"args": "after-used", "argsIgnorePattern": "^_"}]
    },
    "globals": {
        "angular": "readonly",
        "moment": "readonly",
        "io": "readonly",
        "nginxPath": "readonly",
        "iziToast": "readonly"
    }
}
