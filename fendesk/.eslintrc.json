{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "mocha": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "comma-dangle": ["error", "never"],
        "no-unused-vars": [2, {"args": "after-used", "argsIgnorePattern": "^_"}],
        "prefer-destructuring": ["error", {"object": true, "array": false}]
    }
  }
  