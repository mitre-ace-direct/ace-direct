  "description": "This folder is not required for the Provider portal.",
  "main": "app.js",
  "scripts": {
    "test": "mocha --exit",
    "clean": "rm -rf node_modules >/dev/null 2>&1 || true",
    "clean:logs": "rm logs/*.log* > /dev/null 2>&1 || true",
