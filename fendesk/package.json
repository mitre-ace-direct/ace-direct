    "build": "npm run clean && npm install"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "body-parser": "^1.15.0",
    "clear": "0.0.1",
    "express": "^4.13.4",
    "https": "^1.0.0",
    "ip": "^1.1.5",
    "log4js": "^3.0.6",
    "minimist": "^1.2.5",
    "mocha": "^8.3.0",
    "mysql": "^2.10.2",
    "nconf": "^0.8.4",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1"
  }
