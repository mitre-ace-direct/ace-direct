{
  "name": "ace-direct-kurento",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "DEBUG=ace:*,JsSIP:* nodemon --watch src --exec node src/index",
    "build": "node ./build.js && npm run sequelize db:migrate",
    "build2": "node ./build2.js && npm run sequelize db:migrate",
    "clean": "rm -rf node_modules >/dev/null && rm -rf vendor/kurento-client-js/node_modules && rm -rf vendor/kurento-jsonrpc/node_modules && rm -rf vendor/reconnect-ws/node_modules && rm -rf call-metrics/node_modules && rm -rf ACEKurento-js/node_modules && rm -rf static/bower_components",
    "clean:logs": "rm logs/*.log* > /dev/null 2>&1 || true",
    "postinstall": "cd static && bower install",
    "sequelize": "sequelize",
    "lint": "echo 'TODO'",
    "bower": "cd static && bower install"
  },
  "dependencies": {
    "asterisk-manager": "0.1.16",
    "aws-sdk": "^2.382.0",
    "cors": "^2.8.5",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "event-stream": "^3.3.5",
    "express": "^4.16.4",
    "jssip": "^3.2.15",
    "jssip-node-websocket": "^3.0.0",
    "kind-of": "^6.0.3",
    "kurento-client": "./vendor/kurento-client-js",
    "minimist": "^1.2.3",
    "mysql2": "^1.6.4",
    "nconf": "^0.11.2",
    "param": "^1.1.0",
    "passport": "^0.4.0",
    "sequelize": "^4.41.2",
    "util": "0.10.3",
    "uuid": "^3.3.2",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "bower": "^1.8.8",
    "nodemon": "^1.19.1",
    "sequelize-cli-no-vulnerability": "latest"
  },
  "description": "A Web and signaling server application that uses Kurento Media Server to make/receive one to one calls to/from Asterisk",
  "main": "build.js",
  "author": "",
  "license": "ISC"
}
